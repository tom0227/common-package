{
  "name": "@tom0227/shared-modules",
  "version": "1.3.0",
  "description": "Shared modules for ORI Packaging microservices",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist",
    "prepare:disabled": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "lint:check": "eslint \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "nestjs",
    "shared",
    "modules",
    "microservices"
  ],
  "author": "ORI Development Team",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "restricted"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tom0227/common-package.git",
    "directory": "shared-modules"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/config": "^3.0.0 || ^4.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/graphql": "^12.0.0 || ^13.0.0",
    "@nestjs/jwt": "^10.0.0 || ^11.0.0",
    "@nestjs/passport": "^10.0.0 || ^11.0.0",
    "@prisma/client": "^6.0.0",
    "axios": "^1.4.0",
    "ioredis": "^5.3.2",
    "jwks-rsa": "^3.0.1",
    "nest-winston": "^1.10.2",
    "passport-jwt": "^4.0.1",
    "redis": "^5.8.2",
    "uuid": "^13.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.9",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-prettier": "^4.2.5",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
    "prisma": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.4.4",
    "typescript": "^5.1.3"
  }
}
